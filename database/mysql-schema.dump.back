CREATE DEFINER=`root`@`localhost` PROCEDURE `update_sub_folio_void_by_breakdown1`(
        IN breakdown1x BIGINT(20),
        IN void_by VARCHAR(20),
        IN void_reason VARCHAR(250),
        IN user_id VARCHAR(20)
    )
BEGIN
  DECLARE done INT DEFAULT 0;
  DECLARE is_pair_with_depositx ENUM("-1","0");
  DECLARE transfer_pair_idx BIGINT(20);
  DECLARE id_logx BIGINT(20);
  DECLARE cursor_transfer_pair CURSOR FOR SELECT is_pair_with_deposit, transfer_pair_id, id_log FROM sub_folio WHERE breakdown1 = breakdown1x;
  DECLARE CONTINUE HANDLER FOR SQLSTATE '02000' SET done = 1;

  OPEN cursor_transfer_pair;

  REPEAT
    FETCH cursor_transfer_pair INTO is_pair_with_depositx, transfer_pair_idx, id_logx;
    IF NOT done THEN
      CALL delete_acc_foreign_cash_by_transaction(id_logx, 2, user_id);
      IF is_pair_with_depositx = "0" THEN
        CALL update_sub_folio_void(transfer_pair_idx, void_by, void_reason, user_id);
      ELSE
        CALL update_guest_deposit_void(transfer_pair_idx, void_by, void_reason, user_id);
      END IF;
    END IF;
  UNTIL done END REPEAT;

  CLOSE cursor_transfer_pair;

  UPDATE sub_folio SET
    void = "-1",
    void_date = NOW(),
    void_by = void_by,
    void_reason = void_reason,
    user_id = user_id
  WHERE breakdown1 = breakdown1x;
END ;;
