CREATE DEFINER=`root`@`localhost` PROCEDURE `delete_pos_product_costing`(
        IN check_numberx VARCHAR(15),
        IN user_id VARCHAR(20)
    )
BEGIN
  DECLARE done INT DEFAULT 0;
  DECLARE costing_numberx VARCHAR(12);
  DECLARE cursor_costing CURSOR FOR SELECT costing_number FROM pos_product_costing WHERE check_number=check_numberx;
  DECLARE CONTINUE HANDLER FOR SQLSTATE '02000' SET done = 1;

  OPEN cursor_costing;

  REPEAT
    FETCH cursor_costing INTO costing_numberx;
    IF NOT done THEN
      CALL delete_inv_costing(costing_numberx, user_id);
    END IF;
  UNTIL done END REPEAT;

  CLOSE cursor_costing;

  UPDATE pos_product_costing SET user_id = user_id WHERE check_number = check_numberx;
  DELETE FROM pos_product_costing WHERE check_number = check_numberx;
END ;;
