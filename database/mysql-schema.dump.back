CREATE DEFINER=`root`@`localhost` PROCEDURE `delete_invoice_payment`(
        IN invoice_numberx VARCHAR(15),
        IN user_id VARCHAR(20)
    )
BEGIN
  DECLARE done INT DEFAULT 0;
  DECLARE ref_numberx VARCHAR(20);
  DECLARE sub_folio_idx BIGINT(20);
  DECLARE amountx DECIMAL(15,3);
  DECLARE cursor_invoice_payment CURSOR FOR SELECT ref_number, sub_folio_id, amount FROM invoice_payment WHERE invoice_number=invoice_numberx;
  DECLARE CONTINUE HANDLER FOR SQLSTATE '02000' SET done = 1;

  OPEN cursor_invoice_payment;

  REPEAT
    FETCH cursor_invoice_payment INTO ref_numberx, sub_folio_idx, amountx;
    IF NOT done THEN
      CALL delete_acc_journal(ref_numberx, user_id);
      CALL update_invoice_item_amount_paid(sub_folio_idx, -amountx, user_id);
      CALL delete_acc_foreign_cash_by_transaction(sub_folio_idx, 31, user_id);
    END IF;
  UNTIL done END REPEAT;

  CLOSE cursor_invoice_payment;

  UPDATE invoice_payment SET user_id = user_id WHERE invoice_number = invoice_numberx;
  DELETE FROM invoice_payment WHERE invoice_number = invoice_numberx;
END ;;
DELIMITER ;
