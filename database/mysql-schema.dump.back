CREATE DEFINER=`root`@`localhost` FUNCTION `balance_add`(code VARCHAR(200),
        amount DECIMAL(15,3)
    ) RETURNS decimal(15,3)
BEGIN
  DECLARE codevar VARCHAR(200);
  DECLARE amountvar DOUBLE;

  IF IFNULL(@amountvar, 0) = 0 THEN SET @amountvar := 0; END IF;
  IF IFNULL(@codevar, "XXXNULLXXX") <> code THEN
    SET @amountvar := 0;
    SET @codevar := code;
  END IF;

  SET @amountvar := @amountvar + amount;

  RETURN @amountvar;
END ;;
