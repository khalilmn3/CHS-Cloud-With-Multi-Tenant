CREATE DEFINER=`root`@`localhost` PROCEDURE `update_inv_purchase_order_is_received`(
        IN numberx VARCHAR(12),
        IN user_id VARCHAR(20)
    )
BEGIN
  UPDATE inv_purchase_order SET
    is_received = IF(IFNULL((
      SELECT
        COUNT(po_number) AS A
      FROM
        inv_purchase_order_detail
        WHERE inv_purchase_order_detail.po_number = numberx
        AND (quantity_received + quantity_not_received) < quantity),0)>0,
      IF(IFNULL((
      	SELECT
          SUM(inv_purchase_order_detail.quantity_received)
        FROM
         inv_purchase_order_detail
        WHERE inv_purchase_order_detail.po_number = numberx),0)>0, "1", "0"),
      "-1"),
    user_id = user_id
  WHERE number = numberx;
END ;;
