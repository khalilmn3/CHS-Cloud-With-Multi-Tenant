CREATE DEFINER=`root`@`localhost` PROCEDURE `delete_acc_ap_ar_payment_by_ref_number`(
        IN ref_numberx VARCHAR(20),
        IN user_id VARCHAR(20)
    )
BEGIN
  DECLARE done INT DEFAULT 0;
  DECLARE ap_ar_numberx VARCHAR(12);
  DECLARE amountx DECIMAL(15,3);
  DECLARE cursor_amount CURSOR FOR SELECT ap_ar_number, amount FROM acc_ap_ar_payment_detail WHERE ref_number = ref_numberx;
  DECLARE CONTINUE HANDLER FOR SQLSTATE '02000' SET done = 1;

  OPEN cursor_amount;

  REPEAT
    FETCH cursor_amount INTO ap_ar_numberx, amountx;
    IF NOT done THEN
      UPDATE acc_ap_ar SET
        amount_paid = amount_paid - amountx,
        is_paid = "0",
        user_id = user_id
      WHERE number = ap_ar_numberx;
    END IF;
  UNTIL done END REPEAT;

  CLOSE cursor_amount;

  CALL delete_acc_journal(ref_numberx, user_id);
  UPDATE acc_ap_ar_payment_detail SET user_id = user_id WHERE ref_number = ref_numberx;
  DELETE FROM acc_ap_ar_payment WHERE ref_number = ref_numberx;
  UPDATE acc_ap_ar_payment_detail SET user_id = user_id WHERE ref_number = ref_numberx;
  DELETE FROM acc_ap_ar_payment_detail WHERE ref_number = ref_numberx;
END ;;
