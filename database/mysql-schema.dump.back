CREATE DEFINER=`root`@`localhost` PROCEDURE `delete_fa_receive`(
        IN numberx VARCHAR(12),
        IN user_id VARCHAR(20)
    )
BEGIN
  DECLARE done INT DEFAULT 0;
  DECLARE ref_number_receivex VARCHAR(20);
  DECLARE po_numberx VARCHAR(12);
  DECLARE ap_numberx VARCHAR(12);
  DECLARE cursor_ref_number_receive CURSOR FOR SELECT ref_number FROM fa_receive WHERE number=numberx;
  DECLARE cursor_po_number CURSOR FOR SELECT po_number FROM fa_receive WHERE number=numberx;
  DECLARE cursor_ap_number CURSOR FOR SELECT ap_number FROM fa_receive WHERE number=numberx;
  DECLARE CONTINUE HANDLER FOR SQLSTATE '02000' SET done = 1;

  OPEN cursor_ref_number_receive;
  OPEN cursor_po_number;
  OPEN cursor_ap_number;

  FETCH cursor_ref_number_receive INTO ref_number_receivex;
  IF NOT done THEN
    CALL delete_acc_journal(ref_number_receivex, user_id);
  END IF;
  SET done = 0;

  FETCH cursor_po_number INTO po_numberx;
  IF NOT done THEN
    CALL update_fa_purchase_order_is_received(po_numberx, "0", user_id);
  END IF;
  SET done = 0;

  FETCH cursor_ap_number INTO ap_numberx;
  IF NOT done THEN
    CALL delete_acc_ap_ar(ap_numberx, user_id);
  END IF;
  SET done = 0;

  CLOSE cursor_ref_number_receive;
  CLOSE cursor_po_number;
  CLOSE cursor_ap_number;

  CALL delete_fa_list_by_receive_number(numberx, user_id);
  CALL delete_fa_receive_detail(numberx, user_id);
  UPDATE fa_receive SET user_id = user_id WHERE number = numberx;
  DELETE FROM fa_receive WHERE number = numberx;
END ;;
DELIMITER ;
