        IN numberx VARCHAR(12),
        IN user_id VARCHAR(20)
    )
BEGIN
  DECLARE done INT DEFAULT 0;
  DECLARE ref_numberx VARCHAR(20);
  DECLARE cursor_payment_ref_number CURSOR FOR SELECT ref_number FROM acc_ap_ar_payment_detail WHERE ap_ar_number = numberx;
  DECLARE CONTINUE HANDLER FOR SQLSTATE '02000' SET done = 1;

  OPEN cursor_payment_ref_number;

  REPEAT
    FETCH cursor_payment_ref_number INTO ref_numberx;
    IF NOT done THEN
      CALL delete_acc_ap_ar_payment_by_ref_number(ref_numberx, user_id);
    END IF;
  UNTIL done END REPEAT;

  CLOSE cursor_payment_ref_number;

  UPDATE acc_ap_ar SET user_id = user_id WHERE number = numberx;
  DELETE FROM acc_ap_ar WHERE number = numberx;
END ;;
