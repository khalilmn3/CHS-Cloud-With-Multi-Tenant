CREATE DEFINER=`root`@`localhost` PROCEDURE `update_folio_status_cancel_check_in`(
        IN numberx BIGINT(20),
        IN departure DATETIME,
        IN cancel_by VARCHAR(20),
        IN user_id VARCHAR(20)
    )
BEGIN
  DECLARE done INT DEFAULT 0;
  DECLARE guest_detail_idx BIGINT(20);
  DECLARE cursor_guest_detail_id CURSOR FOR SELECT guest_detail_id FROM folio WHERE number = numberx;
  DECLARE CONTINUE HANDLER FOR SQLSTATE '02000' SET done = 1;

  OPEN cursor_guest_detail_id;
  FETCH cursor_guest_detail_id INTO guest_detail_idx;

  UPDATE folio SET
    status_code = "I",
    cancel_date = NOW(),
    cancel_by = cancel_by,
    user_id = user_id
  WHERE number = numberx;

  IF NOT done THEN
    UPDATE guest_detail SET
      departure = departure,
      departure_unixx = UNIX_TIMESTAMP(DATE(departure)),
      user_id = user_id
    WHERE id = guest_detail_idx;
  END IF;

  CLOSE cursor_guest_detail_id;

  CALL update_sub_folio_void_by_folio_number(numberx, cancel_by, "Cancel Check In", user_id);
END ;;
DELIMITER ;
