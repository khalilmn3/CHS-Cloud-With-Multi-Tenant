CREATE DEFINER=`root`@`localhost` PROCEDURE `delete_fa_depreciation`(
        IN numberx VARCHAR(12),
        IN user_id VARCHAR(20)
    )
BEGIN
  DECLARE done INT DEFAULT 0;
  DECLARE ref_numberx VARCHAR(20);
  DECLARE cursor_fa_depreciation CURSOR FOR SELECT ref_number FROM fa_depreciation WHERE number=numberx;
  DECLARE CONTINUE HANDLER FOR SQLSTATE '02000' SET done = 1;

  OPEN cursor_fa_depreciation;

  FETCH cursor_fa_depreciation INTO ref_numberx;
  IF NOT done THEN
    CALL delete_acc_journal(ref_numberx, user_id);
  END IF;

  CLOSE cursor_fa_depreciation;

  CALL delete_fa_revaluation_by_number(numberx, user_id);
  UPDATE fa_depreciation SET user_id = user_id WHERE number = numberx;
  DELETE FROM fa_depreciation WHERE number = numberx;
END ;;
