CREATE DEFINER=`root`@`localhost` PROCEDURE `update_ban_booking_status`(
        IN numberx BIGINT(20),
        IN status_code ENUM('B','I','F','C','N','V'),
        IN user_id VARCHAR(20)
    )
BEGIN
  IF status_code = "B" THEN
    UPDATE `ban_booking` SET
      status_code = status_code,
      change_status_date = "0000-00-00 00:00:00",
      user_id = user_id
    WHERE number = numberx;
  ELSE
    UPDATE `ban_booking` SET
      status_code = status_code,
      change_status_date = NOW(),
      user_id = user_id
    WHERE number = numberx;
  END IF;
END ;;
